{
  "name": "react-fancy-loader",
  "version": "0.1.13",
  "description": "A lightweight set of fancy loaders for your React applications. The package uses ESM, which enables tree shaking.",
  "private": false,
  "sideEffects": false,
  "main": "build/index.esm.js",
  "files": [
    "build/"
  ],
  "module": "build/index.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/amrsekilly/react-fancy-loader.git"
  },
  "keywords": [
    "react",
    "styled-components",
    "load-lab",
    "loadlab",
    "react-loader",
    "spinners",
    "loaders",
    "react-spinner",
    "react-spinners",
    "react-loaders",
    "react-fancy-loader"
  ],
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generateFiles": "rollup -c",
    "deploy": "NODE_ENV=production yarn generateFiles && yarn publish"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^11.3.2",
    "react": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rollup": "^2.15.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0"
  },
  "dependencies": {
    "styled-components": "^5.1.1"
  }
}
